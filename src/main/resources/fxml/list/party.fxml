<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox fx:id="box" alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="76.0" prefWidth="130.0" stylesheets="@../../styles/base.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <VBox>
               <children>
                  <Text fx:id="label" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="list-item-title" text="Text" />
                  <Text fx:id="organizer" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="list-item-subtitle" text="Text" />
                  <Text fx:id="type" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="list-item-subtitle" text="Text" />
                  <Text fx:id="time" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="primary-label" text="Text" />
               </children>
            </VBox>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <JFXButton fx:id="removeButton" styleClass="alert-button-sm" text="REMOVE" />
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
